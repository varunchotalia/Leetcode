# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        if head is None:
            return None
        prev = head
        if head.next is not None:
            nxt = head.next
            prev.next = None
        else:
            return head
        while nxt.next != None:
            temp = nxt.next
            nxt.next = prev
            prev = nxt
            nxt = temp
        nxt.next = prev
        return nxt
            
